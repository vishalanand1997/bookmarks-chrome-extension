{
  "name": "Bookmarks",
  "description": "Here we can add, remove,see the all bookmarks along with search functionalities",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the Bookmark list.."
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "<all_urls>",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "manifest_version": 2
}